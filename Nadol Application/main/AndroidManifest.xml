<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nadol.nadol" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="false"
        android:icon="@drawable/smallmandev"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:hardwareAccelerated="false"
        android:largeHeap="true">

        <activity
            android:name=".checkregistration"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".numberscreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.numberscreen" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".homescreen"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.homescreen" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".newsscreen"
            android:label="@string/manifestnews" >
            <intent-filter>
                <action android:name="android.intent.action.newsscreen" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".specificnews"
            android:label="@string/manifestnews" >
            <intent-filter>
                <action android:name="android.intent.action.specificnews" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sponsors"
            android:label="@string/sponsors" >
            <intent-filter>
                <action android:name="android.intent.action.sponsors" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".settings"
            android:label="@string/settings" >
            <intent-filter>
                <action android:name="android.intent.action.settings" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".feedback"
            android:label="@string/feedbacks" >
            <intent-filter>
                <action android:name="android.intent.action.feedback" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".cricketscreen"
            android:label="@string/npl" >
            <intent-filter>
                <action android:name="android.intent.action.cricketscreen" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".livescore"
            android:label="@string/livescore" >
            <intent-filter>
                <action android:name="android.intent.action.livescore" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".teams"
            android:label="@string/teams" >
            <intent-filter>
                <action android:name="android.intent.action.teams" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fixtures"
            android:label="@string/fixtures" >
            <intent-filter>
                <action android:name="android.intent.action.fixtures" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".results"
            android:label="@string/results" >
            <intent-filter>
                <action android:name="android.intent.action.results" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
        android:name=".MainActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MainActivity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>

        <service
                android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <service
                android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

    </application>

</manifest>
